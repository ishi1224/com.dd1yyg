<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">

        <shape>

            <!--<corners android:radius="5dip" />-->

            <gradient
                android:angle="0"
                android:centerColor="#ff1cbd27"
                android:centerY="0.75"
                android:endColor="#ff1cbd27"
                android:startColor="#ff1cbd27" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">

        <clip>

            <shape>

                <!--<corners android:radius="5dip" />-->

                <gradient
                    android:angle="0"
                    android:centerColor="#801cbd27"
                    android:centerY="0.75"
                    android:endColor="#a01cbd27"
                    android:startColor="#801cbd27" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">

        <clip>

            <shape>

                <!--<corners android:radius="5dip" />-->

                <gradient
                    android:angle="0"
                    android:endColor="#801cbd27"
                    android:startColor="#801cbd27" />
            </shape>
        </clip>
    </item>

</layer-list>